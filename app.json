{
  "expo": {
    "version": "1.0.7",
    "scheme": "routertv",
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/therapod_icon.png",
          "appleTVImages": {
            "icon": "./assets/therapod_icon.png"
          }
        }
      ],
      "expo-router",
      "expo-font",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true
        }
      ],
      "expo-file-system",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "therapod-tv",
          "organization": "iris-ai-5x"
        }
      ]
    ],
    "android": {
      "versionCode": 8,
      "splash": {
        "image": "./assets/images/splash.png"
      },
      "package": "com.ah009.TherapodWellnessTvApp",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ]
    },
    "ios": {
      "splash": {
        "image": "./assets/images/splash.png"
      },
      "bundleIdentifier": "com.ah009.TherapodWellnessTvApp"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "name": "Therapod AI Wellness",
    "slug": "TherapodWellnessTvApp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0e22bb3c-0712-4793-8b20-22e7c16b3c11"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0e22bb3c-0712-4793-8b20-22e7c16b3c11"
    }
  }
}
